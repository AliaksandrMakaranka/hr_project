{
  "name": "hr_company_cursor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "check:dead-code": "ts-prune",
    "check:deps": "depcheck",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky install",
    "check": "npm-run-all --parallel check:*",
    "check:lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"src/**/*.{ts,tsx,css,md}\"",
    "check:dead": "ts-prune",
    "check:deps": "depcheck",
    "check:build": "vite build --mode development",
    "debug": "cross-env DEBUG=* vite",
    "debug:lint": "cross-env DEBUG=eslint:* eslint . --ext ts,tsx --debug",
    "debug:types": "cross-env DEBUG=typescript:* tsc --noEmit --listFiles",
    "debug:build": "cross-env DEBUG=vite:* vite build --mode development",
    "fix": "npm-run-all --parallel fix:*",
    "fix:lint": "eslint . --ext ts,tsx --fix",
    "fix:format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "fix:types": "tsc --noEmit --incremental false"
  },
  "dependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "framer-motion": "^12.16.0",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.513.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.22.3",
    "styled-components": "^6.1.18",
    "swiper": "^11.2.8",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@vitejs/plugin-react": "^4.2.1",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-prune": "^0.10.3",
    "typescript": "~5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-compression2": "^0.14.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,md,json}": [
      "prettier --write"
    ]
  }
}
